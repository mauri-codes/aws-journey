{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "iam:Add*",
                "iam:Attach*",
                "iam:Create*",
                "iam:Delete*",
                "iam:Detach*",
                "iam:Put*",
                "iam:Remove*",
                "iam:Tag*",
                "iam:Untag*",
                "iam:Update*",
                "iam:PassRole",
                "dynamodb:*",
                "lambda:*",
                "sqs:*",
                "s3:*"
            ],
            "Resource": "*"
        }
    ]
}